{
  "name": "mol.core",
  "version": "3.1.0",
  "description": "core file module for mol project",
  "scripts": {
    "dist": "sass scss/core.scss:dist/core.min.css --style compressed",
    "docs": "npm run scss | npm run start",
    "start": "browser-sync start --server 'docs' --files 'docs'",
    "scss": "sass --watch scss/core.scss:docs/core.css --style expanded"
  },
  "keywords": [
    "mol",
    "mol-project",
    "css",
    "sass",
    "scss",
    "scss-module"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mol-project/mol.core.git"
  },
  "author": "flkt <flkt.crnpio@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mol-project/mol.core/issues"
  },
  "devDependencies": {
    "browser-sync": "^2.26.7",
    "mol.normalize": "git+https://github.com/mol-project/mol.normalize.git#v.2.0.3"
  }
}
